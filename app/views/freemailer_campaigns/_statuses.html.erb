<div id="delivery_statuses">
  <table>
    <tr>
      <th>Contact</th>
      <th>Email</th>
      <th>Status</th>
    </tr>
    <% statuses.each do |status| %>
      <tr>
        <td><%= status.contact.name_for_display %></td>
        <td><%= status.contact.primary_email %></td>
        <td><%= status.delivery_status || "Pending" %></td>
      </tr>
    <% end %>
  </table>
  <%= will_paginate(statuses, :renderer => StatusPaginationModalSwitchRenderer) %>
</div>
