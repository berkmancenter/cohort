<h1>Listing freemailer_campaigns</h1>

<table>
  <tr>
    <th>Subject</th>
    <th>Title</th>
    <th>Body template</th>
    <th>Sender</th>
  </tr>

<% @freemailer_campaigns.each do |freemailer_campaign| %>
  <tr>
    <td><%=h freemailer_campaign.subject %></td>
    <td><%=h freemailer_campaign.title %></td>
    <td><%=h freemailer_campaign.body_template %></td>
    <td><%=h freemailer_campaign.sender_id %></td>
    <td><%= link_to 'Show', freemailer_campaign %></td>
    <td><%= link_to 'Edit', edit_freemailer_campaign_path(freemailer_campaign) %></td>
    <td><%= link_to 'Destroy', freemailer_campaign, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New freemailer_campaign', new_freemailer_campaign_path %>