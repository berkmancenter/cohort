<tr class="dnote" id="dnote-<%= dnote.id %>">
  <td><%= link_to(icon(:cross), note_path(:redirect_after => url_for(params), :id => dnote), {:method => :delete, :confirm => 'Are you sure?'}) -%></td>
  <td class="contact"><%= link_to dnote.contact.name_for_display, contact_url(dnote.contact) %></td>
  <td class="priority level-<%= dnote.priority %>"><%= dnote.priority %></td>
  <td class="follow-up"><%= dnote.follow_up %></td>
</tr>
<tr class="note-<%= dnote.id %>">
  <td colspan="3">
    <%= RedCloth.new(dnote.note).to_html %><br/>
    <div class="meta"><strong>Added at:</strong> <%= dnote.created_at %> <strong>By:</strong> <%= dnote.user.username %></strong></div>
  </td>
</tr>
