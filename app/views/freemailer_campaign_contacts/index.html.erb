<h1>Listing freemailer_campaign_contacts</h1>

<table>
  <tr>
    <th>Freemailer campaign</th>
    <th>Contact</th>
    <th>Delivery status</th>
  </tr>

<% @freemailer_campaign_contacts.each do |freemailer_campaign_contact| %>
  <tr>
    <td><%=h freemailer_campaign_contact.freemailer_campaign_id %></td>
    <td><%=h freemailer_campaign_contact.contact_id %></td>
    <td><%=h freemailer_campaign_contact.delivery_status %></td>
    <td><%= link_to 'Show', freemailer_campaign_contact %></td>
    <td><%= link_to 'Edit', edit_freemailer_campaign_contact_path(freemailer_campaign_contact) %></td>
    <td><%= link_to 'Destroy', freemailer_campaign_contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New freemailer_campaign_contact', new_freemailer_campaign_contact_path %>